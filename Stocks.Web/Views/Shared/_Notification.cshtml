@if (TempData.TryGetValue("success", out var successValue))
{
    <script type="text/javascript">
        toastr.success('@successValue');
    </script>
}
@if (TempData.TryGetValue("error", out var errorValue))
{
    <script type="text/javascript">
        toastr.error('@errorValue');
    </script>
}